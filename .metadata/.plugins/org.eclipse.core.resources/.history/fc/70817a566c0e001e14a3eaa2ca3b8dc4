package com.imooc.practice.p2;

public class Waxberry extends Fruits {
	private String color;
	public Waxberry() {
		
	}
	public Waxberry(String shape,String taste,String color) {
		super.setShape(shape);
		super.setTaste(taste);
		this.setColor(color);
	}
