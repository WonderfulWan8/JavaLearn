	public static void testAge() {
		System.out.println("输入年龄：");
		Scanner sc = new Scanner(System.in);
		int age = sc.nextInt();
		if(age<18 || age > 80) {
			System.out.println("需要亲友陪同");
		}
	}
	public static void main(String[] args) {
		
	}
}
